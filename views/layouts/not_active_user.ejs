                                        <span class="collapse-title">
                                            <span class="align-middle navmenu-item">صفحه اصلی</span>
                                        </span>
                                    </div>
                                    <div id="accordion5" role="tabpanel" data-parent="#accordionWrapa2"
                                        aria-labelledby="heading5" class="collapse">
                                        <div class="card-content">
                                            <div class="card-body">
                                                <ul>
                                                    <li><a href="#">صفحه اصلی</a></li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="card collapse-header">
                                    <div id="heading10" class="card-header" data-toggle="collapse" role="button"
                                        data-target="#accordion10" aria-expanded="false" aria-controls="accordion10">
                                        <span class="collapse-title">
                                            <span class="align-middle navmenu-item">تنظیمات</span>
                                        </span>
                                    </div>
                                    <div id="accordion10" role="tabpanel" data-parent="#accordionWrapa2"
                                        aria-labelledby="heading10" class="collapse" aria-expanded="false">
                                        <div class="card-content">
                                            <div class="card-body">
                                                <ul>
                                                    <li><a href="#">تغییر مشخصات فردی </a></li>
                                                    <li><a href="#">تغییر کلمه عبور</a></li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="overley"></div>
                    </div>
                </div>
                <div class="col-9 col-md-9 col-lg-4  ">
                    <div class="logo-personal">
                        <div class="logo-area float-start">
                            <img src="/imgs/logo/logosn.png" width="70px" alt="">
                        </div>
                        <div class="personal-area d-inline-block">

                            <span class="d-inline-block p-1 border-bottom"><%= user_info.name  %> </span>
                            <span class="d-block text-center tag-name"><%= user_info.group_name %> </span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
